{
  "name": "@mm-tech-inc/notion-client",
  "version": "6.12.8",
  "type": "module",
  "description": "Robust TypeScript client for the unofficial Notion API.",
  "repository": "mm-tech-inc/react-notion-x",
  "publishConfig": {
    "@mm-tech-inc:registry": "https://npm.pkg.github.com/"
  },
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "sideEffects": false,
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch --silent --onSuccess 'echo build successful'",
    "test": "ava"
  },
  "dependencies": {
    "got": "^11.8.1",
    "notion-types": "^6.12.6",
    "notion-utils": "^6.12.6",
    "p-map": "^5.3.0"
  },
  "ava": {
    "snapshotDir": ".snapshots",
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--no-warnings",
      "--experimental-specifier-resolution=node"
    ]
  }
}
